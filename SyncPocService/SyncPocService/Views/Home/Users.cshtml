@model IEnumerable<SyncPocService.Models.User>
@{
    ViewData["Title"] = "Users";
}

<br />
<br />
<table class="table table-bordered">
    <thead>
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email</th>
            <th>Phone</th>
            <th>Created At</th>
            <th>Update At</th>
        </tr>
    </thead>
    @foreach (var user in Model)
    {
        <tr>
            <td>@user.FirstName</td>
            <td>@user.LastName</td>
            <td>@user.Email</td>
            <td>@user.Phone</td>
            <td>@user.CreatedAt</td>
            <td>@user.UpdatedAt</td>
        </tr>
    }

</table>
